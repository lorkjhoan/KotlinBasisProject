package edu.unicatolica.kotlin

fun main() {

    var name: String = "jhoan"
    var apellido: String = "rodriguez"
    val edad = 24
    val program = "ing sistemas"
    val intereses = "mejorar cada dia a lo que me dedico, y tratas de disfrutar de todo lo maximo posible :D"

    val message = "hola soy $name $apellido, tengo $edad y estoy en el programa $program y mis intereses es $intereses"
    println(message)

}
